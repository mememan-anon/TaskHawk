{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": "public",
  "functions": {
    "api/*.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/api/search", "destination": "/api/search" },
    { "source": "/api/health", "destination": "/api/health" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
